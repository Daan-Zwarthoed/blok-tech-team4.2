{% extends "layouts/layout.njk" %}
{% block title %}
    Chat
{% endblock %}
{% block content %}
    <div class="chatList">
        <form action="chatSelf/{{ userId }}" method="post">
            {% for user in users %}
                <button name="userOther" value={{ user }}>{{ user }}</button>
            {% endfor %}
        </form>
    </div>
{% endblock %}